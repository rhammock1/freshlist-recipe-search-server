  "dependencies": {
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "helmet": "^4.2.0",
    "knex": "^0.95.1",
    "morgan": "^1.10.0",
    "pg": "^8.5.1",
    "postgrator-cli": "^3.2.0"
